{
  "name": "svelte-select",
  "version": "5.3.0-beta.1",
  "repository": "https://rob-balfre@github.com/rob-balfre/svelte-select.git",
  "description": "A <Select> component for Svelte apps",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:dev & npm run test:browser",
    "test:dev": "rollup -cw",
    "test:browser": "sirv test/public -p 3000 --dev --open",
    "gen:docs": "node docs/generate_theming_variables_md.cjs",
    "preprepare": "node src/remove-styles.cjs",
    "prepare": "svelte-package",
    "postprepare": "node src/post-prepare.cjs && npm run gen:docs",
    "release": "release-it --preRelease=beta"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/adapter-static": "2.0.1",
    "@sveltejs/kit": "1.7.2",
    "@sveltejs/package": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "find-in-files": "^0.5.0",
    "fuse.js": "^6.6.2",
    "prettier": "~2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "release-it": "^15.6.0",
    "rollup": "^3.17.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^7.1.3",
    "sirv-cli": "^2.0.2",
    "svelte-highlight": "^7.2.0",
    "svelte-preprocess": "^5.0.1",
    "svelte-tiny-virtual-list": "^2.0.5",
    "svelte2tsx": "^0.6.1",
    "tape-modern": "^1.1.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.2"
  },
  "author": "Robert Balfr√© <rob.balfre@gmail.com> (https://github.com/rob-balfre)",
  "license": "ISC",
  "keywords": [
    "svelte"
  ],
  "release-it": {
    "hooks": {
      "after:bump": "npm run prepare"
    },
    "github": {
      "release": false
    }
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.2.1",
    "svelte": "^3.55.1",
    "svelte-floating-ui": "1.2.8"
  },
  "exports": {
    "./ChevronIcon.svelte": {
      "types": "./dist/ChevronIcon.svelte.d.ts",
      "svelte": "./dist/ChevronIcon.svelte",
      "default": "./dist/ChevronIcon.svelte"
    },
    "./ClearIcon.svelte": {
      "types": "./dist/ClearIcon.svelte.d.ts",
      "svelte": "./dist/ClearIcon.svelte",
      "default": "./dist/ClearIcon.svelte"
    },
    "./LoadingIcon.svelte": {
      "types": "./dist/LoadingIcon.svelte.d.ts",
      "svelte": "./dist/LoadingIcon.svelte",
      "default": "./dist/LoadingIcon.svelte"
    },
    "./Select.svelte": {
      "types": "./dist/Select.svelte.d.ts",
      "svelte": "./dist/Select.svelte",
      "default": "./dist/Select.svelte"
    },
    "./filter": {
      "types": "./dist/filter.d.ts",
      "default": "./dist/filter.js"
    },
    "./get-items": {
      "types": "./dist/get-items.d.ts",
      "default": "./dist/get-items.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./tailwind.css": "./dist/tailwind.css",
    "./no-styles/ChevronIcon.svelte": {
      "types": "./dist/ChevronIcon.svelte.d.ts",
      "svelte": "./dist/ChevronIcon.svelte",
      "default": "./dist/ChevronIcon.svelte"
    },
    "./no-styles/ClearIcon.svelte": {
      "types": "./dist/ClearIcon.svelte.d.ts",
      "svelte": "./dist/ClearIcon.svelte",
      "default": "./dist/ClearIcon.svelte"
    },
    "./no-styles/LoadingIcon.svelte": {
      "types": "./dist/LoadingIcon.svelte.d.ts",
      "svelte": "./dist/LoadingIcon.svelte",
      "default": "./dist/LoadingIcon.svelte"
    },
    "./no-styles/Select.svelte": {
      "types": "./dist/Select.svelte.d.ts",
      "svelte": "./dist/Select.svelte",
      "default": "./dist/Select.svelte"
    }
  },
  "files": [
    "MIGRATION_GUIDE.md",
    "dist",
    "docs"
  ],
  "svelte": "./dist/index.js"
}
